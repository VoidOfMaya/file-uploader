generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}
//sessiontable: sid varchar/ sess json/ expire timestamp
model Session{
  id      String      @id
  sid     String      @unique
  data    Json
  expire  DateTime    @default(now())
  @@map("session")
}

